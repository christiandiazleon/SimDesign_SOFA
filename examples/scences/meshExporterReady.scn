    <!-- <VisualStyle displayFlags="showForceFields" /> -->
    <Node name="input">
        <Mesh name="surface" filename="../objects/phantomTrial.obj" />
        <MeshTetraStuffing name="stuffing" snapPoints="true" splitTetras="true" draw="false" size="0.7" alphaLong="0.6" alphaShort="0.9" inputPoints="@surface.points" inputTriangles="@surface.triangles" />
        
    </Node>
    <Node name="output">
        <EulerImplicit name="odesolver" />
        <CGLinearSolver iterations="10" name="linear solver" tolerance="1.0e-9" threshold="1.0e-9" />
        <TetrahedronSetTopologyContainer name="volume" points="@../input/stuffing.outputPoints" tetras="@../input/stuffing.outputTetras" />
        <MechanicalObject />
        <!-- Algorithms: used in DiagonalMass to compute the mass -->
        <TetrahedronSetGeometryAlgorithms name="GeomAlgo" />
        <DiagonalMass massDensity="1" name="computed using mass density" />
        <TetrahedralCorotationalFEMForceField name="FEM" youngModulus="4000" poissonRatio="0.3" method="large" />
        <FixedConstraint  name="FixedConstraint" indices="16 124" /> 
        <MeshExporter filename="/home/labrv/Documents/sofa/phantomTrial" format="3" exportAtBegin="true" edges="false" triangles="false" tetras="true" hexas="false" listening="true"/>

